<%- include ("partials/header") %>

<h1>INDEX PAGE</h1>
  
<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>" alt="image1">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href= "/blogs/<%= blog._id %>">Read More</a></<a>
        
    </div>
<% }) %>

<%- include ("partials/footer") %>