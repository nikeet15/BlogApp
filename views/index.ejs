<%- include ("partials/header") %>

<h1>INDEX PAGE</h1>
  
<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%- blog.title %></h2>
        <img src="<%= blog.image %>" alt="image1">
        <span><%= blog.created.toDateString() %></span>
        <% var str= blog.body %>
        <p><%- str.substring(0,20) %>...</p>
        <a href= "/blogs/<%= blog._id %>">Read More</a></<a>
        
    </div>
<% }) %>

<%= include ("partials/footer") %>